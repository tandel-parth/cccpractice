<?php $js = $this->getJs(); ?>
<?php if(count($js)): ?>
    <?php foreach($js as $_js): ?>
        <script src="<?php echo $_js; ?>" ></script>
    <?php endforeach; ?>
<?php endif; ?>

<?php $css = $this->getCss(); ?>
<?php if(count($css)): ?>
    <?php foreach($css as $_css): ?>
        <link rel="stylesheet" href="<?php echo $_css ?>">
    <?php endforeach; ?>
<?php endif; ?>